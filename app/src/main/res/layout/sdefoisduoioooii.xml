<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center_horizontal"
    android:background="@drawable/bac"
    tools:context=".dsfkhfjkdshfjsdk.lfjslkdsjfjdsf.jkdlsjhfjsdhfjki.MainActivity">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:alpha="0.5"
        android:background="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"></FrameLayout>

    <ImageView
        android:id="@+id/TVbanan3"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="30dp"
        android:src="@drawable/kiwi"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/TVlimon"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/TVtextBanan3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2x"
        android:textColor="@color/button"
        android:textSize="50sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/TVbanan2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toEndOf="@+id/TVbanan"
        app:layout_constraintTop_toTopOf="@+id/TVbanan2" />

    <TextView
        android:id="@+id/TVtextBanan2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5x"
        android:textColor="@color/button"
        android:textSize="50sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/TVbanan"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toEndOf="@+id/TVbanan"
        app:layout_constraintTop_toTopOf="@+id/TVbanan" />

    <ImageView
        android:id="@+id/TVbanan2"
        android:layout_width="73dp"
        android:layout_height="73dp"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="30dp"
        android:src="@drawable/fig"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/TVlimon"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/TVlimon"
        android:layout_width="73dp"
        android:layout_height="73dp"
        android:layout_marginStart="40dp"
        android:src="@drawable/papaya"
        app:layout_constraintBottom_toTopOf="@+id/Bok"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/TVtextLimon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3x"
        android:textColor="@color/button"
        android:textSize="50sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/TVlimon"
        app:layout_constraintEnd_toEndOf="@+id/TVtextBanan2"
        app:layout_constraintStart_toStartOf="@+id/TVtextBanan2"
        app:layout_constraintTop_toTopOf="@+id/TVlimon"
        app:layout_constraintVertical_bias="0.793" />

    <ImageView
        android:id="@+id/TVbanan"
        android:layout_width="73dp"
        android:layout_height="73dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/passion_fruit"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TVlimon"
        />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="Winning"
        android:textColor="#FCFDFD"
        android:textSize="60sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Bok"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="56dp"
        android:backgroundTint="#ED9A00"
        android:text="ok"
        android:textColor="@color/button"
        android:textSize="34sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>